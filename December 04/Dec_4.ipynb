    "def pgt(n):\n",
    "    if n==0:\n",
    "        return 0\n",
    "    elif n==1:\n",
    "        return 1\n",
    "    else:\n",
    "        return pgt(n-1)+pgt(n-2)\n",
    "\n",
    "print(pgt(10))\n",
    "print(pgt(12))"
   ]
