    "def josephus(n, k):\n",
    "    if n == 1:\n",
    "        return 1\n",
    "    else:\n",
    "        return (josephus(n - 1, k) + k - 1) % n + 1\n",
    "\n",
    "print(josephus(3,2)) \n",
    "print(josephus(5,3))"
   ]
  }
