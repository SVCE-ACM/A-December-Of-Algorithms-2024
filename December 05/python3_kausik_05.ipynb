   "source": [
    "n = int(input(\"Enter the number of people (n): \"))\n",
    "k = int(input(\"Enter the step count (k): \"))\n",
    "safe= 0\n",
    "for i in range(2, n + 1):\n",
    "    safe=(safe+ k) % i\n",
    "\n",
    "safe += 1\n",
    "print(f\"The safe position is: {safe}\")\n"
   ]
