   "source": [
    "import numpy as np\n",
    "N=int(input(\"Enter no of participants:\"))\n",
    "arr=[]\n",
    "for i in range(N-1):\n",
    "    x=int(input())\n",
    "    arr.append(x)\n",
    "print(\"The participant no returned :\",arr)\n",
    "total=N*(N+1)//2\n",
    "arrtot=np.sum(arr)\n",
    "missingno=total-arrtot\n",
    "print(f\"The participant {missingno} is missing.\")"
   ]
